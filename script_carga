LOAD DATA LOCAL INFILE 'C:\\urls-no-action-taken.csv'
INTO TABLE GOOGLE.urls_no_action_taken
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(REQUEST_ID, DOMAIN, URL);

LOAD DATA LOCAL INFILE 'C:\\requests.csv'
INTO TABLE GOOGLE.REQUESTS
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(REQUEST_ID,DATA_AUX,CHILLING_EFFECTS_URL,COPYRIGHT_OWNER_ID,COPYRIGHT_OWNER_NAME, REPORTING_ORGANIZATION_ID,REPORTING_ORGANIZATION_NAME,URLS_REMOVED,URLS_NO_ACTION,URLS_PENDING_REVIEW);

LOAD DATA LOCAL INFILE 'C:\\domains.csv'
INTO TABLE GOOGLE.DOMAIN
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(REQUEST_ID, DOMAIN, URLS_REMOVED,URLS_NO_ACTION,URLS_PENDING_REVIEW);

LOAD DATA LOCAL INFILE 'C:\\google-government-removal-requests-by-product-and-reason2.csv'
INTO TABLE GOOGLE.removal_requests_prod_reason
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(DIA, SEMESTRE, ANO, PAIS, CD_PAIS, PRODUTO, MOTIVO, MANDADOS, EXECUTIVO_POLICIA_ETC, ITENS_SOLICITADOS_REMOCAO);

LOAD DATA LOCAL INFILE 'C:\\google-government-removal-requests-by-product2.csv'
INTO TABLE GOOGLE.removal_requests_product
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(DIA, SEMESTRE, ANO, PAIS, CD_PAIS, PRODUTO,MANDADOS, EXECUTIVO_POLICIA_ETC, ITENS_SOLICITADOS_REMOCAO);

LOAD DATA LOCAL INFILE 'C:\\grr2.csv'
INTO TABLE GOOGLE.removal_requests_product
FIELDS TERMINATED BY ','
LINES TERMINATED BY '\n'
(DIA, SEMESTRE, ANO, PAIS, CD_PAIS, PRODUTO,MANDADOS, EXECUTIVO_POLICIA_ETC, ITENS_SOLICITADOS_REMOCAO);



